    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "ua-parser-js": "^0.7.21"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "istanbul": "^0.4.5",
    "mocha": "^7.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0"
  }
