{
  "name": "express-ua-redirect",
  "version": "1.0.4",
  "description": "Express UA Redirect is a simple configurable middleware for ExpressJS allowing you to redirect all routes to a configured route based on user agent restrictions.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/world-gaming-fed/express-ua-redirect.git"
  },
  "scripts": {
    "pretest": "node_modules/.bin/eslint ./*.js ./lib/*.js ./lib/**/*.js",
    "test": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcov ./*.spec.js **/*.spec.js **/**/*.spec.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "express",
    "user-agent",
    "ua",
    "not",
    "compatible",
    "browser",
    "redirect",
    "middleware"
  ],
